	<!-- Local Include References -->
	<link rel="stylesheet" href="/css/normalize.css?v=0.1">
	<link rel="stylesheet" href="/css/global.css?v=0.1">
	<script src="/js/global.js"></script>
</head>
<body>

	<!-- ---------------- Navigation Bar ---------------- -->
	
